<div id="<%= dom_id tweet %>">
  <p>
    <strong>Body:</strong>
    <%= tweet.body %>
  </p>

  <p>
    <strong>User:</strong>
    <%= tweet.user %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= tweet.created_at %>
  </p>

  <p>
    <strong>Likes:</strong>
    <%= tweet.likes %>
  </p>

</div>
