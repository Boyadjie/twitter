<p style="color: green"><%= notice %></p>

<h1>Hashtags to tweets</h1>

<div id="hashtags_to_tweets">
  <% @hashtags_to_tweets.each do |hashtags_to_tweet| %>
    <%= render hashtags_to_tweet %>
    <p>
      <%= link_to "Show this hashtag to tweet", hashtags_to_tweet %>
    </p>
  <% end %>
</div>

<%= link_to "New hashtag to tweet", new_hashtags_to_tweet_path %>
